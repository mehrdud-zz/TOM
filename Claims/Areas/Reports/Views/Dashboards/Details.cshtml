@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Home/WillisMaster_CRUD.cshtml";
}

<div class="editor-field">
    <a target="_blank" href=" @( String.Format("{0}{1}:{2}/Reports/Dashboards/SharePointDashboard?DashboardId={3}", (HttpContext.Current.Request.IsSecureConnection ? "https://" : "http://"), HttpContext.Current.Request.Url.Host, ((HttpContext.Current.Request.Url.Port == 80) ? String.Empty : HttpContext.Current.Request.Url.Port.ToString()), ViewBag.DashboardId)) ">        SharePoint URL
    </a>
</div>
<br />

<div id="dash" style="position:relative;"></div>
 <script type="text/javascript">



     function DrawEveryThing() {
         cftDashboard.LoadDashboard(cftDashboard);
     }

     var dashboardId = GetQueryStringValueOOP("DashboardId");
     var reportId = GetQueryStringValueOOP("ReportId");
     var cftDashboard = new CFTDashboard(dashboardId, "dash", reportId);

     if (google != null) {
         google.setOnLoadCallback(DrawEveryThing);
     }



