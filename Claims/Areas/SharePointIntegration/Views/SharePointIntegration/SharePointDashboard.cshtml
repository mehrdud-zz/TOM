@model ModelsLayer.ReportTemplate
@{
    ViewBag.Title = "SharePointDashboard";
}
<!DOCTYPE html>
<html>
<head>
    <title></title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <link type="text/css" rel="stylesheet" href="/Content/Styles/ChartPainter.css" />

    <script type="text/javascript" src="//www.google.com/jsapi"></script>


    <script type="text/javascript">
      

    </script>

    <script type="text/javascript" src="/Content/Scripts/ChartPainter.js"></script> 

</head>
<body>
    <div id="frameCotnainer"></div>
    <script type="text/javascript">
        function drawDashboard(){
            var reportId = @(Model.ReportID);
            var width = Math.floor(document.documentElement.clientWidth);
            var height = Math.floor(document.documentElement.clientHeight); 
            drawMyChart(reportId, 0, 0, width, height, "Frame"+reportId , "@(Model.Name)", 0);
        }

        google.load('visualization', '1.1', { packages: ['controls'] }); 
        google.setOnLoadCallback(drawDashboard);
    </script>
</body>
</html> 